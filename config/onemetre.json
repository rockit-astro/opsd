{
  "daemon": "onemetre_operations",
  "log_name": "opsd@onemetre",
  "control_machines": ["OneMetreDome", "OneMetreTCS", "OneMetreDAS"],
  "pipeline_machines": ["OneMetreDAS"],
  "actions_module": "rockit.operations.actions.onemetre",
  "scripts_module": "rockit.operations.scripts.onemetre",
  "loop_delay": 10,
  "site_latitude": "28.7603135 N",
  "site_longitude": "17.8796168 W",
  "site_elevation": 2387,
  "sun_altitude_limit": 5,
  "dome": {
    "module": "rockit.operations.dome.astrohaven",
    "daemon": "onemetre_dome",
    "movement_timeout": 120,
    "heartbeat_timeout": 119
  },
  "environment_daemon": "observatory_environment",
  "environment_conditions": [
    {
      "label": "Wind",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "wind_speed"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "wind_speed"
        },
        {
          "label": "0.5m",
          "sensor": "halfmetre_vaisala",
          "parameter": "wind_speed"
        }
      ]
    },
    {
      "label": "Median Wind",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "median_wind_speed"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "median_wind_speed"
        },
        {
          "label": "0.5m",
          "sensor": "halfmetre_vaisala",
          "parameter": "median_wind_speed"
        }
      ]
    },
    {
      "label": "Temperature",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "temperature"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "temperature"
        },
        {
          "label": "0.5m",
          "sensor": "halfmetre_vaisala",
          "parameter": "temperature"
        }
      ]
    },
    {
      "label": "Humidity",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "relative_humidity"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "relative_humidity"
        },
        {
          "label": "0.5m",
          "sensor": "halfmetre_vaisala",
          "parameter": "relative_humidity"
        }
      ]
    },
    {
      "label": "Int. Humidity",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_domealert",
          "parameter": "internal_humidity"
        }
      ]
    },
    {
      "label": "Dew Point",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "dew_point_delta"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "dew_point_delta"
        },
        {
          "label": "0.5m",
          "sensor": "halfmetre_vaisala",
          "parameter": "dew_point_delta"
        }
      ]
    },
    {
      "label": "Rain",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "rain",
          "parameter": "unsafe_boards"
        },
        {
          "label": "0.5m",
          "sensor": "halfmetre_vaisala",
          "parameter": "rg11_unsafe"
        }
      ]
    },
    {
      "label": "Sec. Sys.",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_domealert",
          "parameter": "security_system_safe"
        }
      ]
    },
    {
      "label": "Network",
      "sensors": [
        {
          "label": "Google",
          "sensor": "netping",
          "parameter": "google"
        },
        {
          "label": "NGTSHead",
          "sensor": "netping",
          "parameter": "ngtshead"
        }
      ]
    },
    {
      "label": "UPS Battery",
      "sensors": [
        {
          "label": "UPS1",
          "sensor": "w1m_power",
          "parameter": "ups1_battery_remaining"
        },
        {
          "label": "UPS2",
          "sensor": "w1m_power",
          "parameter": "ups2_battery_remaining"
        }
      ]
    },
    {
      "label": "Disk Space",
      "sensors": [
        {
          "label": "Bytes",
          "sensor": "w1m_diskspace",
          "parameter": "data_fs_available_bytes"
        }
      ]
    },
    {
      "label": "Sun",
      "sensors": [
        {
          "label": "Altitude",
          "sensor": "ephem",
          "parameter": "sun_alt"
        }
      ]
    }
  ]
}
