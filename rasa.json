{
  "daemon": "rasa_operations",
  "log_name": "rasa_opsd",
  "control_machines": ["RASAMain"],
  "pipeline_machines": ["RASAMain"],
  "actions_module": "warwick.rasa.operations.actions",
  "loop_delay": 10,
  "site_latitude": "28.7603135 N",
  "site_longitude": "17.8796168 W",
  "site_elevation": 2387,
  "dome_daemon": "rasa_dome",
  "dome_open_timeout": 120,
  "dome_close_timeout": 120,
  "dome_heartbeat_timeout": 119,
  "environment_daemon": "observatory_environment",
  "environment_conditions": [
    {
      "label": "Wind",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "wind_speed"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "wind_speed"
        }
      ]
    },
    {
      "label": "Median Wind",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "median_wind_speed"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "median_wind_speed"
        }
      ]
    },
    {
      "label": "Temperature",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "temperature"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "temperature"
        }
      ]
    },
    {
      "label": "Humidity",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "relative_humidity"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "relative_humidity"
        }
      ]
    },
    {
      "label": "Int. Humidity",
      "sensors": [
        {
          "label": "RASA",
          "sensor": "rasa_internal",
          "parameter": "relative_humidity"
        }
      ]
    },
    {
      "label": "Dew Point",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "w1m_vaisala",
          "parameter": "dew_point_delta"
        },
        {
          "label": "GOTO",
          "sensor": "goto_vaisala",
          "parameter": "dew_point_delta"
        }
      ]
    },
    {
      "label": "Rain",
      "sensors": [
        {
          "label": "W1m",
          "sensor": "rain",
          "parameter": "unsafe_boards"
        }
      ]
    },
    {
      "label": "Network",
      "sensors": [
        {
          "label": "Google",
          "sensor": "netping",
          "parameter": "google"
        },
        {
          "label": "NGTSHead",
          "sensor": "netping",
          "parameter": "ngtshead"
        }
      ]
    },
    {
      "label": "UPS",
      "sensors": [
        {
          "label": "Status",
          "sensor": "rasa_power",
          "parameter": "ups_status"
        },
        {
          "label": "Battery",
          "sensor": "rasa_power",
          "parameter": "ups_battery_remaining"
        }
      ]
    },
    {
      "label": "Disk Space",
      "sensors": [
        {
          "label": "Bytes",
          "sensor": "rasa_diskspace",
          "parameter": "data_fs_available_bytes"
        }
      ]
    },
    {
      "label": "Sun",
      "sensors": [
        {
          "label": "Altitude",
          "sensor": "ephem",
          "parameter": "sun_alt"
        }
      ]
    }
  ]
}
